<Window x:Class="GAS.Screens.Add_Cmd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GAS.Screens"
        mc:Ignorable="d"
        Title="Add_Cmd" Height="148.813" Width="589.71">
    <Grid>
        <TextBox x:Name="command" HorizontalAlignment="Left" Height="23" Margin="10,17,0,0" TextWrapping="Wrap" Text="Command" VerticalAlignment="Top" Width="476" SpellCheck.IsEnabled="True"/>
        <TextBox x:Name="index" HorizontalAlignment="Left" Height="23" Margin="491,17,0,0" TextWrapping="Wrap" Text="Index" VerticalAlignment="Top" Width="82"/>
        <TextBox x:Name="comment" HorizontalAlignment="Left" Height="23" Margin="10,45,0,0" TextWrapping="Wrap" Text="Comment" VerticalAlignment="Top" Width="476"/>
        <Button x:Name="cancel" Content="Cancel" HorizontalAlignment="Left" Margin="498,89,0,0" VerticalAlignment="Top" Width="75" Click="cancel_Click"/>
        <Button x:Name="add" Content="Add" HorizontalAlignment="Left" Margin="418,89,0,0" VerticalAlignment="Top" Width="75" Click="add_Click"/>

    </Grid>
</Window>
